{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "lint": {
            "outputs": []
        },
        "test": {
            "outputs": []
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "storybook": {
            "cache": false,
            "persistent": true
        },
        "build-storybook": {
            "outputs": [
                "storybook-static/**"
            ]
        }
    }
}
